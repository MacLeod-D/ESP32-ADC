void loop() {
  for (int i=5;i<256;i++) {  
      dac_output_voltage(DAC_CHANNEL_1, (i) & 0xff);
      adcStart(35);
      delayMicroseconds(200);
      a1 = adcEnd(35);
      a1 = (int)ADC_LUT[a1];  // Correct the value
      Serial.print(i*16); Serial.print(" "); Serial.println(a1);
